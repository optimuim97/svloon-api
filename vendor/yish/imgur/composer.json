{
    "name": "yish/imgur",
    "description": "Imgur upload API with Laravel.",
    "keywords": ["laravel", "upload", "imgur"],
    "homepage": "https://github.com/Mombuyish/Laravel-Imgur",
    "type": "library",
    "authors": [
        {
            "name": "Yish",
            "email": "mombuartworks@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Yish\\Imgur\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "Yish\\Imgur\\UploadServiceProvider"
            ],
            "aliases": {
                "Imgur": "Yish\\Imgur\\Facades\\Upload"
            }
        }
    }
}
